	<rdfs:domain rdf:resource="&OrganisationalModel;DomainOntologyElement"/>
	<rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:about="&OrganisationalModel;enactedBy">
	<rdfs:domain rdf:resource="&OrganisationalModel;Role"/>
	<rdfs:range rdf:resource="&OrganisationalModel;User"/>
	<a:inverseProperty rdf:resource="&OrganisationalModel;enactsRole"/>
</rdf:Property>
<rdf:Property rdf:about="&OrganisationalModel;enactsRole"
	 rdfs:comment="all Roles a user is able to enact">
	<rdfs:range rdf:resource="&OrganisationalModel;Role"/>
	<rdfs:domain rdf:resource="&OrganisationalModel;User"/>
	<a:inverseProperty rdf:resource="&OrganisationalModel;enactedBy"/>
</rdf:Property>
<rdf:Property rdf:about="&OrganisationalModel;experienceLevel"
	 a:allowedValues="novice"
	 a:defaultValues="novice"
	 a:maxCardinality="1"
	 a:minCardinality="1"
