</rdf:Property>
<rdf:Property rdf:about="&OrganisationalModel;containsMember">
	<rdfs:domain rdf:resource="&OrganisationalModel;OrganisationalUnit"/>
	<a:allowedClasses rdf:resource="&OrganisationalModel;Role"/>
	<a:allowedClasses rdf:resource="&OrganisationalModel;User"/>
	<rdfs:range rdf:resource="&OrganisationalModel;__OrganisationalModel__"/>
	<a:inverseProperty rdf:resource="&OrganisationalModel;belongsToUnit"/>
</rdf:Property>
<rdf:Property rdf:about="&OrganisationalModel;describedBy"
	 rdfs:comment="specifies the information need by its potential content">
	<a:allowedClasses rdf:resource="&OrganisationalModel;InformationDescription"/>
	<rdfs:domain rdf:resource="&OrganisationalModel;InformationNeed"/>
	<a:allowedClasses rdf:resource="&OrganisationalModel;Interest"/>
	<rdfs:range rdf:resource="&OrganisationalModel;__OrganisationalModel__"/>
</rdf:Property>
<rdf:Property rdf:about="&OrganisationalModel;description"
