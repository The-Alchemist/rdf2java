	<rdfs:domain rdf:resource="&OrganisationalModel;Group"/>
	<rdfs:range rdf:resource="&OrganisationalModel;OrganisationalUnit"/>
	<rdfs:domain rdf:resource="&OrganisationalModel;Project"/>
</rdf:Property>
